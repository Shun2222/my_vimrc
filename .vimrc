set number
set fileencodings=utf-8,cp932
set cursorline
syntax enable
set hlsearch
set incsearch
set smartindent
set clipboard+=unnamedset clipboard+=unnamed
set laststatus=2
inoremap <silent> jj <ESC>
set expandtab
set tabstop=4
set shiftwidth=4
set ignorecase
set smartcase
set autoindent
set ruler
set path+=$PWD/**
colorscheme pablo
